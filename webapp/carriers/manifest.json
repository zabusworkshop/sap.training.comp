{
	"_version": "1.7.0",
	"sap.app": {
		"id": "sap.training.comp.carriers",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"TravelService": {
				"uri": "/sap/opu/odata/sap/ZTRAVEL_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.training.comp.carriers.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.training.comp.carriers.i18n.i18n"
				}
			},
			"": {
				"dataSource": "TravelService"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"viewPath": "sap.training.comp.carriers.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"clearControlAggregation": true,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"name": "main",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["main"]
			}, {
				"name": "airline",
				"pattern": "airline/{Carrid}",
				"titleTarget": "",
				"greedy": false,
				"target": ["Airline"]
			}],
			"targets": {
				"main": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Main"
				},
				"Airline": {
					"viewType": "XML",
					"viewName": "Airline"
				},
				"notFound": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "NotFound"
				}
			}
		}
	}
}